1545229608762:(defn other-media-placeholder [path-range]\n    (for [path path-range]\n        [:img {:src (rand-supplement-image)}]))
1545229624499:(defn rand-supplement-image\n  []\n  (str "https://picsum.photos/" \n        (->> (js/Math.random)\n            (* 30)\n            (js/Math.floor)\n            (- (* 0.2 (.-width js/screen))))\n        "/?random"))\n)
